    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:comment xml:lang="en">An event that occurs and concludes at some time before this event.</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/vocab/bio/0.1/Event"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <rdfs:label xml:lang="en">Preceding Event</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/vocab/bio/0.1/Event"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#differentFrom"/>
    <dc:issued>2010-05-06</dc:issued>
    <ns2:plural xml:lang="en">Preceding Events</ns2:plural>
  </owl:TransitiveProperty>

  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/Interval">
    <rdfs:comment xml:lang="en">A extended interval of time related to a particular state of affairs, such as the lifespan of a person or a period of employment.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/bio/0.1/"/>
    <rdfs:label xml:lang="en">Interval</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2006/time#ProperInterval"/>
    <dc:issued>2010-05-03</dc:issued>
    <ns1:term_status>unstable</ns1:term_status>
  </owl:Class>

  <owl:Class rdf:about="http://purl.org/vocab/bio/0.1/Formation">
